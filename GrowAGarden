local Main = Window:NewTab("Open")
local MainSection = Main:NewSection("Boton")
--inicio
MainSection:NewToggle("Gear", "Shop", function(state)
    local player = game.Players.LocalPlayer
    local gearShop = player:WaitForChild("PlayerGui"):FindFirstChild("Gear_Shop")

    if gearShop then
        if state then
            -- Habilita la interfaz
            gearShop.Enabled = true
            print("Toggle On: Gear_Shop habilitada")
        else
            -- Deshabilita la interfaz
            gearShop.Enabled = false
            print("Toggle Off: Gear_Shop deshabilitada")
        end
    else
        print("Error: Gear_Shop no encontrado")
    end
end)
--new
-- Interruptor para Seed_Shop
MainSection:NewToggle("Seed Shop", "Shop", function(state)
    local player = game.Players.LocalPlayer
    local seedShop = player:WaitForChild("PlayerGui"):FindFirstChild("Seed_Shop")

    if seedShop then
        if state then
            -- Habilita la interfaz
            seedShop.Enabled = true
            print("Toggle On: Seed_Shop habilitada")
        else
            -- Deshabilita la interfaz
            seedShop.Enabled = false
            print("Toggle Off: Seed_Shop deshabilitada")
        end
    else
        print("Error: Seed_Shop no encontrado")
    end
end)
--new
MainSection:NewToggle("Event Shop", "Shop", function(state)
    local player = game.Players.LocalPlayer
    local eventShop = player:WaitForChild("PlayerGui"):FindFirstChild("EventShop_UI")

    if eventShop then
        if state then
            -- Habilita la interfaz
            eventShop.Enabled = true
            print("Toggle On: EventShop_UI habilitada")
        else
            -- Deshabilita la interfaz
            eventShop.Enabled = false
            print("Toggle Off: EventShop_UI deshabilitada")
        end
    else
        print("Error: EventShop_UI no encontrado")
    end
end)
--end
