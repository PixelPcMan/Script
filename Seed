local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/PixelPcMan/Lubra/refs/heads/main/Ui"))()
local Window = Library.CreateLib("Pixel Farm", "DarkTheme")
--Main
local Main = Window:NewTab("Buy")
local MainSection = Main:NewSection("Free")
--inicio
MainSection:NewToggle("BuyBanana", "Auto", function(state)
    if state then
        _G.on = true
        while _G.on do
local args = {
    [1] = "Banana"
}

game:GetService("ReplicatedStorage").GameEvents.BuySeedStock:FireServer(unpack(args))
            wait(1)
        end
    else
        _G.on = false
    end
end)
--new
