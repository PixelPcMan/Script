local Main = Window:NewTab("Buy")
local MainSection = Main:NewSection("Main")
--inicio
MainSection:NewToggle("Banana", "Auto", function(state)
    if state then
        _G.on = true
        while _G.on do
local args = {
    [1] = "Banana"
}

game:GetService("ReplicatedStorage").GameEvents.BuySeedStock:FireServer(unpack(args))
            wait(1)
        end
    else
        _G.on = false
    end
end)
--end
MainSection:NewToggle("SoulFruit", "Auto", function(state)
    if state then
        _G.on = true
