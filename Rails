local Main = Window:NewTab("Recarga")
local MainSection = Main:NewSection("RecargaAutomatica")
--inicio
MainSection:NewToggle("Armas", "Auto", function(state)
    if state then
        _G.on = true
        while _G.on do
loadstring(game:HttpGet("https://raw.githubusercontent.com/PixelPcMan/LibryRaild/refs/heads/main/Recarga"))()
            wait(0.1)
        end
    else
        _G.on = false
    end
end)
--end
local Main = Window:NewTab("Drop")
local MainSection = Main:NewSection("Maim")
--inicio
MainSection:NewToggle("Drop", "Auto", function(state)
    if state then
        _G.on = true
        while _G.on do
            game:GetService("ReplicatedStorage").Remotes.DropItem:FireServer()
            wait(0.1)
        end
    else
        _G.on = false
    end
end)
--new
MainSection:NewToggle("Hide", "Auto", function(state)
    if state then
        _G.on = true
        while _G.on do
loadstring(game:HttpGet("https://raw.githubusercontent.com/PixelPcMan/Universal/refs/heads/main/Hideguis"))()
            wait()
        end
    else
        _G.on = false
    end
end)
--new
MainSection:NewButton("Hide", " ", function()
-- LocalScript

-- Funci√≥n para ocultar todas las GUIs
local function hideGuis()
    -- Obtiene el jugador local
    local player = game.Players.LocalPlayer
    -- Itera sobre todas las GUIs en el PlayerGui
    for _, gui in pairs(player.PlayerGui:GetChildren()) do
        if gui:IsA("ScreenGui") then
            gui.Enabled = false -- Oculta la GUI
        end
    end
end

-- Llama a la funci√≥n para ocultar las GUIs
hideGuis()
end)
--end
MainSection:NewButton("Drop", " ", function()
game:GetService("ReplicatedStorage").Remotes.DropItem:FireServer()
end)
--end
